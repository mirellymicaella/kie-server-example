{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Launch App",
            "request": "launch",
            "mainClass": "App",
            "projectName": "assingnment"
        },
        {
            "type": "java",
            "name": "Launch Test",
            "request": "launch",
            "mainClass": "com.senseup.drools.firefighter.Test",
            "projectName": "drools-training"
        },
        {
            "type": "java",
            "name": "Launch ServerApplication",
            "request": "launch",
            "mainClass": "com.drools.server.ServerApplication",
            "projectName": "server"
        },
        {
            "type": "java",
            "name": "Launch BootJbpmKieServerApplication",
            "request": "launch",
            "mainClass": "com.github.mnadeem.kie.BootJbpmKieServerApplication",
            "projectName": "boot-jbpm-kie-server"
        },
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "enter program name, for example ${workspaceFolder}/a.out",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}